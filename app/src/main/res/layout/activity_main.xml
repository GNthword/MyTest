<?xml version="1.0" encoding="utf-8"?>
<com.milog.test.mytest.common.TestView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >


            <TextView
                android:id="@+id/tv_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <Button
                android:id="@+id/btn_layout_test"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="nubia test"
                />

            <Button
                android:id="@+id/btn_list_test"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="list test"
                />

            <Button
                android:id="@+id/btn_grid_view_test"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="gridview test"
                />

            <Button
                android:id="@+id/btn_relative_test"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="relative test"
                />

            <Button
                android:id="@+id/btn_permission_test"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="permission test"
                />

            <Button
                android:id="@+id/btn_function_test"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="function test"
                />


            <Button
                android:id="@+id/btn_aspectj_test"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="aspectj test"
                />


            <Button
                android:id="@+id/btn_aa_test"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="aa test"
                />


            <Button
                android:id="@+id/btn_wv_test"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="webview test"
                />


        </LinearLayout>

    </ScrollView>



</com.milog.test.mytest.common.TestView>
